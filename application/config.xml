<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns       ="http://www.w3.org/ns/widgets"
        xmlns:gap      ="http://phonegap.com/ns/1.0"
		id          ="com.livrezonpam.app"
		versioncode ="1"
		version     ="1.0.0">
    <access origin="*" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="whatsapp:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <gap:plugin name="pushbots-cordova-plugin" spec="1.6.x" source="npm" />
 <preference name="android-minSdkVersion" value="16" />
<preference name="android-targetSdkVersion"  value="26" />

<preference name="android-build-tool" value="gradle" />
<preference name="EnableViewportScale" value="false" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
		<name>livrezonpam</name>

		<description>
		    Aplikasyon livrezon ant patikilye
		</description>

		<author href="http://kwenpam.com/livrezonpam" email="kwenpam2017@gmail.com"></author>
		<preference name="orientation" value="portrait"/>
		<icon src="logo.png"/>
    <preference name="stay-in-webview" value="true" />
	<preference name="permissions"  value="none"/>

    <!--<plugin name="cordova-plugin-media" /> -->

    <access origin="*.pushbots.com" />
    <access origin=".*" />
    <allow-navigation href="http://www.kwenpam.com/*" />

	<!-- <gap:plugin name="cordova-plugin-customurlscheme" source="npm">
	<param name="URL_SCHEME" value="livrezonpam" />
	</gap:plugin> -->
</widget>
